@model int

<div id="loadPhotoForm">
    @using (Html.BeginForm("AddCategoryPhoto", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <input type="hidden" name="categoryId" value="@Model" />
        <div class="form-group ">
            <label for="image">Загрузить</label>
            <input name="image" id="file1" type="file" accept="image/*,image/jpeg"
                   title=" выбрать файл" />
        </div>
        <div class="form-group">
            <button class="btn btn-success" type="submit">Сохранить</button>
        </div>
    }
</div>
